{
    "image":"mcr.microsoft.com/devcontainers/universal:2",

    // Configure tool-specific properties.
    "customizations": {
        "vscode": {
            "extensions": [
                "esbenp.prettier-vscode",
                "vscode-icons-team.vscode-icons",
                "wayou.vscode-todo-highlight"
            ],
            "settings": {
                "editor.fontSize": 16,
                "terminal.integrated.fontSize": 16,
                "editor.wordWrap": "on"
            }
            
        }
        
    }
}